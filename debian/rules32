#!/usr/bin/make -f
# -*- makefile -*-

# Uncomment this to turn on verbose mode.
export DH_VERBOSE=1

%:
	dh  $@

override_dh_auto_configure:
	mkdir build
	cd build
	/opt/Qt5.15.0/bin/qmake -o Makefile s3Browser.pro -spec linux-g++-32 CONFIG+=release

override_dh_auto_build:
	make -j2

override_dh_install:
	dh_install
